<script>
    function EditAnnouncementImage(id) {
        $.ajax({
            type: "POST",
            url: '/Announcement/EditAnnouncementImage?id=' + id,
            success: function (data) {
                debugger
                for (var i = 0; i < data['data'].length; i++) {
                    var urlX = "http://admin.xtanbulplus.com"+  data['data'][i]['Photos'];
                    toDataURL(urlX, data['data'][i]['Photos'],
                        function (dataUrl) {
                            console.log('RESULT:', dataUrl)
                        }
                    );
                }
            },
            error: function (request, status, error) {
                swal.fire("Hata!", "Bir sorun ile karşılaşıldı!", "error");
            }
        });
    };
</script>
<script>
    function toDataURL(src, name, callback, outputFormat) {
        var imgWrap = "";
        imgWrap = $(".upload__img-wrap");
        debugger
        let image = new Image();
        image.crossOrigin = 'Anonymous';

        image.src = src;
        if (image.complete || image.complete === undefined) {
            image.src = "data:image/gif;base64, R0lGODlhAQABAIAAAAAAAP///ywAAAAAAQABAAACAUwAOw==";
            image.src = src;
            imgArrayForRealEstate.push(image);
        }
        var html =
            "<div class='upload__img-box' id='MultiImage'><div style='background-image: url(" +
            image.src +
            ")' data-number='" + 20 +
            "' data-file='" +
            name +
            "' class='img-bg'><div class='upload__img-close'></div></div></div>";
        imgWrap.append(html);
        imgArrayForRealEstateArr.push(name);
    }
</script>
<script>

    jQuery(document).ready(function () {
        ImgUpload();
    });
    function ImgUpload() {
        var imgWrap = "";

        $(".upload__inputfile").each(function () {
            $(this).on("change", function (e) {
                imgWrap = $(this).closest(".upload__box").find(".upload__img-wrap");
                var maxLength = $(this).attr("data-max_length");

                var files = e.target.files;
                var filesArr = Array.prototype.slice.call(files);
                var iterator = 0;
                filesArr.forEach(function (f, index) {
                    debugger
                    if (!f.type.match("image.*")) {
                        return;
                    }

                    if (imgArrayForRealEstate.length > maxLength) {
                        return false;
                    } else {
                        var len = 0;
                        for (var i = 0; i < imgArrayForRealEstate.length; i++) {
                            if (imgArrayForRealEstate[i] !== undefined) {
                                len++;
                            }
                        }
                        if (len > maxLength) {
                            return false;
                        } else {
                            imgArrayForRealEstate.push(f);

                            var reader = new FileReader();
                            reader.onload = function (e) {
                                var html =
                                    "<div class='upload__img-box' id='MultiImage'><div style='background-image: url(" +
                                    e.target.result +
                                    ")' data-number='" +                               
                                    f.name +
                                    "' class='img-bg'><div class='upload__img-close'></div></div></div>";
                                imgWrap.append(html);
                                iterator++;
                            };
                            reader.readAsDataURL(f);
                        }
                    }
                });
            });
        });
        $("body").on("click", ".upload__img-close", function (e) {
            debugger
            var file = $(this).parent().data("file");
            for (var i = 0; i < imgArrayForRealEstate.length; i++) {
                if (imgArrayForRealEstate[i].name === file) {
                    imgArrayForRealEstate.splice(i, 1);
                    break;
                }
            }
            for (var i = 0; i < imgArrayForRealEstateArr.length; i++) {
                if (imgArrayForRealEstateArr[i] === file) {
                    imgArrayForRealEstateArr.splice(i, 1);
                    break;
                }
            }
            $(this).parent().parent().remove();
        });
    };
</script>