@using Entity
@model RealEstates
@{
    Teams currentAdmins = Session["currentUser"] as Teams;
    int usercheck = 1;
    int type = 0;
    if (currentAdmins == null)
    {
        usercheck = 0;
    }
    else
    {   // Bu kısım current user boş değilse kısmı olsun
        if (currentAdmins.UserType == UserType.User)
        {
            type = 1;
        }
        else if (currentAdmins.UserType == UserType.Admin)
        {
            type = 2;
        }
    }
    Layout = "~/Views/Shared/_Layout.cshtml";


}
<input type="number" value="@type" id="UserType" hidden disabled />
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
    @*begin::Toolbar*@
    <div class="toolbar py-2" id="kt_toolbar">
        <!--begin::Container-->
        <div id="kt_toolbar_container" class="container-fluid d-flex align-items-center">
            <!--begin::Page title-->
            <div class="flex-grow-1 flex-shrink-0 me-5">
                <!--begin::Page title-->
                <div data-kt-swapper="true" data-kt-swapper-mode="prepend" data-kt-swapper-parent="{default: '#kt_content_container', 'lg': '#kt_toolbar_container'}" class="page-title d-flex align-items-center flex-wrap me-3 mb-5 mb-lg-0">
                    <!--begin::Title-->
                    <h1 class="d-flex align-items-center text-dark fw-bolder my-1 fs-3">
                       Danışmanların İlanları
                    </h1>
                    <!--end::Title-->
                </div>
                <!--end::Page title-->
            </div>
            <!--end::Page title-->
        </div>
        <!--end::Container-->
    </div>

    @*end::Toolbar*@
    <!--begin::Container-->
    <div id="kt_content_container" class="container-xxl">
        <!--begin::Card-->
        <div class="card">
            <!--begin::Card header-->
            <!--<div class="card-header border-0 pt-6">-->
                <!--begin::Card title-->
                <!--<div class="card-title">
                    <a href="/Announcement/AddAnnouncement" class="btn btn-primary">İlan Ekle</a>
                </div>-->
                <!--begin::Card title-->
            <!--</div>-->
            <!--end::Card header-->
            <!--begin::Card body-->
            <div class="card-body pt-0">
                <!--begin::Table-->
                <table class="table align-middle table-row-dashed fs-6 gy-5" id="kt_table_AnnouncementList">
                    <!--begin::Table head-->
                    <thead>
                        <!--begin::Table row-->
                        <tr class="text-start text-gray-400 fw-bolder fs-7 text-uppercase gs-0">
                            <th class="min-w-125px">S/N</th>
                            <th class="min-w-125px">Fotoğraf</th>
                            <th class="min-w-125px">Başlık</th>
                            <th class="min-w-125px">İlan Durumu</th>
                            <th class="min-w-125px">Danışman Ad</th>
                            <th class="min-w-125px">Danışman Ünvan</th>
                            <th class="min-w-125px">Fiyat</th>
                            <th class="min-w-125px">İl</th>
                            <th class="min-w-125px">İlçe</th>
                            <th class="min-w-125px">İşlemler</th>
                        </tr>
                        <!--end::Table row-->
                    </thead>
                    <!--end::Table head-->
                    <!--begin::Table body-->
                    <!--end::Table body-->
                </table>
                <!--end::Table-->
            </div>
            <!--end::Card body-->
        </div>

    </div>
    <!--end::Container-->
</div>

<script src="~/assets/vendors/general/jquery/dist/jquery.js" type="text/javascript"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
